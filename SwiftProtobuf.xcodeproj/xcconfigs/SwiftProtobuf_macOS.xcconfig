// Architectures
SDKROOT = macosx

// Packaging
INFOPLIST_FILE = SwiftProtobuf.xcodeproj/Protobuf_Info.plist
PRODUCT_BUNDLE_IDENTIFIER = org.swift.protobuf.SwiftProtobuf
PRODUCT_MODULE_NAME = SwiftProtobuf
