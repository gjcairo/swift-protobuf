// AppleInternal Section


// Architectures
SDKROOT = macosx.internal

// Build Options
BUILD_LIBRARY_FOR_DISTRIBUTION = YES
SUPPORTS_TEXT_BASED_API = YES
SWIFT_INSTALL_OBJC_HEADER = NO

